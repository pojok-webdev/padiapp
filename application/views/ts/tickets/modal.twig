<div id="modal-add-ticket" class="modal hide fade" tabindex="-1">
	<div class="modal-header no-padding">
		<div class="table-header">
			<button type="button" class="close" data-dismiss="modal">&times;</button>
			Penambahan Tiket
		</div>
	</div>
	<div class="modal-body no-padding_">
		<div class="row-fluid">
			<label for="form-field-mask-3">
				Pelanggan
			</label>
			<div class="input-prepend">
				<span class="add-on">
					<i class="fa fa-child"></i>
				</span>
				<input class="input-medium input-mask-product" type="text" id="client" placeholder="Nama Pelanggan" />
			</div>
			<div class="control-group">
				<label class="control-label" for="form-field-1">Cabang</label>			
				<div class="controls">
					<span class="add-on">
						<i class="fa fa-sitemap"></i>
					</span>
					<input type="text" id="site" placeholder="Nama Cabang" />
				</div>
			</div>
		</div>
		<label for="form-field-mask-3">
			Keluhan
		</label>
		<div class="input-prepend">
			<span class="add-on">
				<i class="icon-comment"></i>
			</span>
			<input class="input-medium input-mask-product" type="text" id="subject" placeholder="Keluhan" />
		</div>
		<label for="form-field-mask-3">
			Pelapor
		</label>
		<div class="input-prepend">
			<span class="add-on">
				<i class="icon-user"></i>
			</span>
			<input class="input-medium input-mask-product" type="text" id="reporter" placeholder="Nama Pelapor" />
		</div>
		<label for="form-field-mask-3">
			Telepon
			<small class="text-error">a*-999-a999</small>
		</label>
		<div class="input-prepend">
			<span class="add-on">
				<i class="icon-phone"></i>
			</span>
			<input class="input-medium input-mask-product" type="text" id="phone" placeholder="No. Telepon"/>
		</div>
	</div>
	<div class="modal-footer">
		<button class="btn btn-small btn-danger pull-left" data-dismiss="modal">
			<i class="icon-remove"></i>
			Tutup
		</button>
	</div>
</div>
